@media (max-width: 690px) {
	table, thead, tbody, th, td, tr { 
		display: block; 
	}
	/* Hide table headers (but not display: none;, for accessibility) */
	thead tr { 
		position: absolute;
		top: -9999px;
		left: -9999px;
	}
	
	td { 
		/* Behave  like a "row" */
		border: none;
		border-bottom: 1px solid #eee; 
		position: relative;
		padding-left: 50%; 
	}
	
	td:before { 
		/* Now like a table header */
		position: absolute;
		/* Top/left values mimic padding */
		top: 0;
		left: 10px;
		width: 25%; 
		padding-right: 10px; 
		white-space: nowrap;
	}
	.space-setting-content table.time-slot-table .daystring, .space-setting-content table.time-slot-table td, table.time-slot-table td.checkbutton-cell, table.time-slot-table td.checkbutton-cell {
		padding-left: 30%; 
		width: 100%;
		line-height: 40px;
	}
	.space-setting-content table:not(.ui-datepicker-calendar) tr {
    height: auto;
	}
	.inplaceedit .edit span {
		display: block;
		    padding-left: 0;
	}
	table.time-slot-table td.checkbutton-cell .checkmark {
    margin-top: 12px;
	}
	.display.hour-column {
    padding-left: 0;
}
/*
