    background-image: linear-gradient(to bottom,#e6e6e6,#fdfdfd);
    background-repeat: repeat-x;
    filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#e6e6e6', endColorstr='#fdfdfd', GradientType=0);
    box-shadow: inset 0 1px 2px rgba(0,0,0,.1);
    border-radius: 4px;
}
.progress .bar {
    width: 0;
    height: 18px;
    color: #fff;
    font-size: 12px;
    text-align: center;
    text-shadow: 0 -1px 0 rgba(0,0,0,.25);
    background-color: #0e90d2;
    background-image: -webkit-gradient(linear,0 0,0 100%,from(rgba(250,190,70,1)),to(rgba(254,182,69,1)));
    background-image: -webkit-linear-gradient(top,rgba(250,190,70,1),rgba(254,182,69,1));
    background-image: linear-gradient(to bottom,rgba(250,190,70,1),rgba(254,182,69,1));
    background-repeat: repeat-x;
    filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='rgba(250,190,70,1)', endColorstr='rgba(254,182,69,1)', GradientType=0);
    box-shadow: inset 0 -1px 0 rgba(0,0,0,.15);
    -ms-box-sizing: border-box;
    box-sizing: border-box;
    -webkit-transition: width .6s ease;
    transition: width .6s ease;
}
.progress-info .bar {
    background-color: #4bb1cf;
    background-image: -webkit-gradient(linear,0 0,0 100%,from(rgba(241,231,103,1)),to(rgba(252,222,70,1)));
