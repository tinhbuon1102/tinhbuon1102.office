[data-tooltip] {
    position: relative;
    display: inline-block;
}
[data-tooltip]:after, [data-tooltip]:before {
    position: absolute;
    -webkit-transform: translate3d(0,0,0);
    transform: translate3d(0,0,0);
    visibility: hidden;
    opacity: 0;
    z-index: 1010;
    pointer-events: none;
    -webkit-transition: .3s ease;
    transition: .3s ease;
    -webkit-transition-delay: 0ms;
    transition-delay: 0ms;
}
[data-tooltip]:before {
    content: "";
    position: absolute;
    background: 0 0;
    border: 7px solid transparent;
    z-index: 1011;
}
[data-tooltip]:after, [data-tooltip]:before {
    position: absolute;
    -webkit-transform: translate3d(0,0,0);
    transform: translate3d(0,0,0);
    visibility: hidden;
    opacity: 0;
    z-index: 1010;
    pointer-events: none;
    -webkit-transition: .3s ease;
    transition: .3s ease;
    -webkit-transition-delay: 0ms;
    transition-delay: 0ms;
}
.HomepageAuth-form-tooltipWrapper {
    display: block;
}
[data-tooltip]:after {
    box-shadow: 0 0 24px rgba(0,0,0,.2);
}
[data-tooltip]:after {
    content: attr(data-tooltip);
    background: #1f2836;
    color: #F7F7F7;
    border-radius: 4px;
    padding: 10px 10px;
    font-size: 13px;
    font-family: inherit;
    font-weight: 400;
    line-height: 13px;
    white-space: nowrap;
}
.HomepageAuth-form-tooltipWrapper::after, .HomepageAuth-form-tooltipWrapper::before {
    position: absolute;
    top: 0;
    left: 100%;
    z-index: 1000;
}
.HomepageAuth-form-tooltipWrapper::before {
    bottom: 0;
    left: 100%;
    width: 0;
    height: 0;
    margin: auto;
    border-top: 4px solid transparent;
    border-right: 4px solid #0e1724;
    border-bottom: 4px solid transparent;
}
.HomepageAuth-form-tooltipWrapper::after, .HomepageAuth-form-tooltipWrapper::before {
    position: absolute;
    top: 0;
    left: 100%;
